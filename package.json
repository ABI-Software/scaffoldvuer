{
  "name": "@abi-software/scaffoldvuer",
  "version": "0.4.0-vue3.6",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/ABI-Software/scaffoldvuer.git"
  },
  "scripts": {
    "serve": "vite --host --force",
    "build-bundle": "vite build",
    "build-static": "vite build -c vite.static-build.js",
    "styleguide": "vue-cli-service styleguidist",
    "styleguide:build": "vue-cli-service styleguidist:build",
    "changelog": "auto-changelog -p --output CHANGELOG.md --template keepachangelog",
    "version": "npm run build-bundle;npm run changelog; git add CHANGELOG.md",
    "release:beta": "npm version prerelease --preid=beta; npm publish --tag beta",
    "release:minor": "npm version minor; npm publish",
    "release:patch": "npm version patch;  npm publish"
  },
  "type": "module",
  "main": "./dist/scaffoldvuer.umd.cjs",
  "module": "./dist/scaffoldvuer.js",
  "exports": {
    ".": {
      "import": "./dist/scaffoldvuer.js",
      "require": "./dist/scaffoldvuer.umd.cjs"
    },
    "./dist/style.css": "./dist/style.css",
    "./src/*": "./src/*"
  },
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "@abi-software/flatmapvuer": "^0.6.0-vue3.2",
    "@abi-software/svg-sprite": "^0.4.0-vue3-beta.0",
    "@element-plus/icons-vue": "^2.3.1",
    "@vue/compat": "^3.4.15",
    "axios": "^0.21.2",
    "core-js": "^3.22.5",
    "current-script-polyfill": "^1.0.0",
    "element-plus": "^2.4.4",
    "minisearch": "^6.0.1",
    "query-string": "^6.11.1",
    "simple-dropzone": "^0.8.3",
    "unplugin-vue-components": "^0.26.0",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue3-component-svg-sprite": "^0.0.1",
    "zincjs": "^1.3.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.2",
    "auto-changelog": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "base64-inline-loader": "^2.0.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "sass": "^1.69.5",
    "vite": "^5.0.10",
    "vue-styleguidist": "^4.72.4",
    "webpack-node-externals": "^2.5.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
